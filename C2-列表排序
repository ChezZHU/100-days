# -*- coding: utf-8 -*-
L = [8,2,50,3]
L2 = sorted (L,key=abs)
print(L2)
# 第2题：列表排序

# -*- coding: utf-8 -*-
L = [8,2,50,3,1]
print(sorted(L))
# 第2题：列表排序 解2

# -*- coding: utf-8 -*-
L = [8,2,50,3]
print(sorted(L))
L = [4, 2, 25, 7777777, 100, 3, 77777777, 77777777, 77777777, 77777777]
print(sorted(L))

# 解3
L = [8,2,50,3]
L.sort()
print(L)

# 注意sorted和sort的用法
